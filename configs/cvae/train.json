{
    "model": {
        "type": "ConditionalVAE",
        "args": {
            "n_channels": 3,
            "num_classes": 10,
            "latent_dim": 512,
            "img_size": 256
        }
    },
    "data": { 
        "train": {
            "type": "ArtBench10_256x256",
            "dataset_args": {
                "root": "/kaggle/input/real-ai-art/Real_AI_SD_LD_Dataset",
                "train": true
            },
            "dataloader_args": {
                "batch_size": 16,
                "num_workers": 1,
                "shuffle": true
            }
        },
        "test": {
            "type": "ArtBench10_256x256",
            "dataset_args": {
                "root": "/kaggle/input/real-ai-art/Real_AI_SD_LD_Dataset",
                "train": false,
                "limit": 2500
            },
            "dataloader_args": {
                "batch_size": 20,
                "num_workers": 1,
                "shuffle": false
            }
        }
    },
    "optimizer": {
        "lr": 1e-3,
        "weight_decay": 1e-6
    },
    "lr_scheduler": {
        "linear_warmup": {
            "warmup_iters": 25000
        },
        "exponential": {
            "gamma": 0.9996
        }
    },
    "trainer": {
        "epochs": 10,
        "iterations_per_epoch": 10000
    },
    "wandb": {
        "project": "dl2-generation"
    }
}